#layout-menu
{
	position: absolute;
	top: 94px;
	left: 0;
	width: 170px;
	height: 603px;
	background: transparent url(http://www.anidb.net/css/anidbstyle/images/bg/menuback.png) no-repeat top left;
}

#layout-menu div.menu
{
	padding: 52px 42px 0px 29px;
}

#layout-menu div.usermenu
{
	position: absolute;
	top: 420px;
	left: 10px;
}

#layout-menu a:link, #layout-menu a:visited {
	color: #ffffff;
}

#layout-menu a:active {
	color: #ff0000;
}
#layout-menu *, #layout-menu a:hover {
	color: #bbbbbb;
}

#layout-menu ul
{
	list-style: none outside;
	margin: 0;
	padding: 0;
	text-indent: 0;
	line-height: 1.2;
}

#layout-menu li
{
	list-style: none outside;
	margin: 0;
	padding: 0;
	text-indent: 0;
}

#layout-menu li.mymessages, #layout-menu li.mynotifies, #layout-menu li.mywishlist,
#layout-menu li.myvotes, #layout-menu li.myreviews, #layout-menu li.mydbentries,
#layout-menu li.mycreqs, #layout-menu li.up2date, #layout-menu li.mydbentries
{
	display: none;
}

#layout-menu li.main, #layout-menu li.latest, #layout-menu li.genre,
#layout-menu li.forum, #layout-menu li.report
{
	margin-bottom: 1.2em;
}

#layout-menu li.category, #layout-menu li.category
{
	display: inline;
}

#layout-menu li.help
{
	padding: 1px 0 0 0;
	margin-bottom: 2px;
	display: block;
	text-indent: 15px;
	background-position: left;
	background-repeat: no-repeat;
	background-image: url(http://www.anidb.net/css/anidbstyle/images/ask2.gif);
}

#layout-menu li.help a:after
{
	content:" / howto";
}

#layout-menu li.reclinks a:before
{
	content:"Rec. ";
}

#layout-menu li.forum
{
